Ext.Ajax.defaultHeaders = {'Accept': 'application/json'};
Ext.require([
    'Ext.direct.Manager',
    'Ext.direct.RemotingProvider'
], function() {
{% for namespace,actions in apis %}
Ext.direct.Manager.addProvider({
    "url": "{{ url(route, {'bundle': namespace})|raw }}",
    "type": "remoting",
    "actions": {{ actions|json_encode|raw }},
    "namespace": "{{ namespace }}"
});
{% endfor %}
});